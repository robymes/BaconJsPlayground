{
  "name": "BaconJsPlayground",
  "version": "0.0.1",
  "description": "BaconJS Playground",
  "authors": [
    "Roberto Messora"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
