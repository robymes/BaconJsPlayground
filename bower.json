    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "v3.3.5",
    "bootstrap-datepicker": "v1.4.0",
    "jquery": "2.1.4",
    "bacon": "0.7.83",
    "bacon.jquery": "0.4.12",
    "bacon.model": "0.1.12",
    "moment": "2.10.6",
    "linqjs": "3.0.5",
    "maquette": "v2.0.0"
